{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "MaaS state schemas",
  "definitions": {
    "bookingState": {
      "description": "The life-cycle state of a booking",
      "type": "string",
      "enum": [
        "START",
        "PENDING",
        "PAID",
        "RESERVED",
        "CONFIRMED",
        "ACTIVATED",
        "ON_HOLD",
        "CANCELLED",
        "EXPIRED",
        "REJECTED",
        "FINISHED"
      ]
    },
    "legState": {
      "description": "The life-cycle state of a leg",
      "type": "string",
      "enum": [
        "START",
        "PLANNED",
        "PAID",
        "ACTIVATED",
        "CANCELLED",
        "FINISHED"
      ]
    },
    "itineraryState": {
      "description": "The life-cycle state of an itinerary",
      "type": "string",
      "enum": [
        "START",
        "PLANNED",
        "PAID",
        "ACTIVATED",
        "CANCELLED",
        "CANCELLED_WITH_ERRORS",
        "FINISHED"
      ]
    }
  }
}
