{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/maas-backend/geocoding/geocoding-reverse/response",
  "description": "MaaS.fi geocoding (GeoJSON) response schema",
  "type": "object",
  "properties": {
    "type": {
      "enum": [
        "FeatureCollection"
      ]
    },
    "features": {
      "type": "array",
      "items": {
        "allOf": [
          {
            "properties": {
              "type": {
                "enum": [
                  "Feature"
                ]
              },
              "geometry": {
                "type": "object",
                "properties": {
                  "type": {
                    "enum": [
                      "Point"
                    ]
                  },
                  "coordinates": {
                    "description": "Geographic latitude-longitude number-pair array in WGS-84 system, see https://en.wikipedia.org/wiki/World_Geodetic_System",
                    "type": "array",
                    "items": [
                      {
                        "description": "Geographic latitude (north-south axis) in WGS-84 system, see https://en.wikipedia.org/wiki/World_Geodetic_System",
                        "type": "number",
                        "minimum": -90,
                        "maximum": 90
                      },
                      {
                        "description": "Geographic longitude (east-west axis) in WGS-84 system, see https://en.wikipedia.org/wiki/World_Geodetic_System",
                        "type": "number",
                        "minimum": -180,
                        "maximum": 180
                      }
                    ]
                  }
                },
                "additionalProperties": false,
                "required": [
                  "type",
                  "coordinates"
                ]
              },
              "properties": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 256
                  },
                  "streetNumber": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 16
                  },
                  "streetName": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 256
                  },
                  "city": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 64
                  },
                  "country": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 64
                  },
                  "countryCode": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 256
                  },
                  "houseNumber": {
                    "type": "integer",
                    "minLength": 1,
                    "maxLength": 16
                  },
                  "zipcode": {
                    "type": "integer"
                  }
                },
                "required": [
                  "name"
                ]
              }
            },
            "additionalProperties": false,
            "required": [
              "type",
              "geometry",
              "properties"
            ]
          },
          {
            "type": "object",
            "properties": {
              "properties": {
                "type": "object",
                "required": [
                  "city",
                  "country",
                  "countryCode"
                ]
              }
            }
          }
        ]
      }
    },
    "debug": {
      "type": "object"
    }
  },
  "required": [
    "type",
    "features"
  ],
  "additionalProperties": false
}